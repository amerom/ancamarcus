<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Anca Marcus Fitness{% endblock %}</title>
        <!-- Responsive Metatag -->
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />

        <!-- <meta http-equiv="X-UA-Compatible" content="IE=9" />-->
        {% block stylesheets %}
        <!-- ****************************  CSS  **************************** -->
        <!-- Fonts: Open Sans and Raleway, from Google Fonts -->
        <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800%7CRaleway:400,100,200,300,500,600,800,700,900' />

        <!-- Font icons: Fontello -->
        <link rel="stylesheet" href="{{ asset('assets/css/vendors/fontello.css') }}" />

        <!-- Bootstrap -->
        <link rel="stylesheet" href="{{ asset('assets/css/vendors/bootstrap.min.css') }}" />

        <!-- Owl Carousel -->
        <link rel="stylesheet" href="{{ asset('assets/js/vendors/owl-carousel/owl.carousel.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/js/vendors/owl-carousel/owl.theme.css') }}">

        <!-- Slider Revolution -->
        <link rel="stylesheet" href="{{ asset('assets/js/vendors/revolution/css/settings.css') }}">

        <!-- Custom Scroll Bars -->
        <link rel="stylesheet" href="{{ asset('assets/js/vendors/malihu/jquery.mCustomScrollbar.css') }}" />

        <!-- Custom CSS -->
        <link rel="stylesheet" href="{{ asset('assets/css/styles.css') }}" />
        <link rel="stylesheet" href="{{ asset('assets/css/colors/orange.css') }}" />
        <link rel="stylesheet" href="{{ asset('build/app.css') }}" />
        <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">

        <![endif]-->

        {% endblock %}

    </head>



    <body>

    <!-- begin Header -->
    <header>

    {% include 'Header/_topbar.html.twig' %}

    {% block header %} {% endblock %}

    </header>
    <!-- end Header -->

        {% block body %}

        {% endblock %}



    <!-- begin Footer -->
    <footer>
        {% include 'Footer/_hours.html.twig' %}
        {% include 'Footer/_info.html.twig' %}
        {% include 'Footer/_copy.html.twig' %}
    </footer>
    <!-- end Footer -->

    {% block javascripts %}

        <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->

        <!-- Media queries -->
        <!--[if lt IE 9]>
        <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>

        <!-- jQuery -->
        <script src="{{ asset('assets/js/vendors/jquery/jquery-1.11.1.min.js') }}"></script>

        <!-- Respond.js media queries for IE8 -->
        <script src="{{ asset('assets/js/vendors/respond.min.js') }}"></script>

        <!-- Bootstrap-->
        <script src="{{ asset('assets/js/vendors/bootstrap.min.js') }}" ></script>

        <!-- Owl Carousel -->
        <script src="{{ asset('assets/js/vendors/owl-carousel/owl.carousel.js') }}"></script>

        <!-- Slider Revolution -->
        <script src="{{ asset('assets/js/vendors/revolution/js/jquery.themepunch.plugins.min.js') }}"></script>
        <script src="{{ asset('assets/js/vendors/revolution/js/jquery.themepunch.revolution.min.js') }}"></script>

        <!-- Custom Scroll Bars -->
        <script src="{{ asset('assets/js/vendors/malihu/jquery.mCustomScrollbar.concat.min.js') }}"></script>

        <!-- Placeholder.js -->
        <!--[if lte IE 9]> <script src="{{ asset('js/vendors/placeholder.js') }}" ></script> <script>Placeholder.init();</script> <![endif]-->

        <!-- Gmaps -->
        <script src="http://maps.google.com/maps/api/js?sensor=true" ></script>
        <script src="{{ asset('assets/js/vendors/gmaps.js') }}" ></script>

        <script src="{{ asset('assets/js/vendors/bootstrap-slider.js') }}" ></script>
        <script src="{{ asset('assets/js/vendors/jquery/jquery.nav.js') }}" ></script>

        <!-- Custom -->
        <script src="{{ asset('assets/js/script.js') }}"  ></script>


        {% endblock %}
    </body>
</html>
